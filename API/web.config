<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<connectionStrings>
		<add name="BarkeeperContext" connectionString="Host=localhost;Database=barkeeper-development;Port=5432;User ID=root;Password=Password01!" providerName="Npgsql" />
	</connectionStrings>
	<entityFramework>
		<providers>
			<provider invariantName="Npgsql" type="Npgsql.NpgsqlServices, EntityFramework6.Npgsql" />
		</providers>
		<!-- setting the default connection factory is optional -->
		<defaultConnectionFactory type="Npgsql.NpgsqlConnectionFactory, EntityFramework6.Npgsql" />
	</entityFramework>
	<system.data>
		<DbProviderFactories>
			<add name="Npgsql Provider" invariant="Npgsql" description=".NET Framework Data Provider for PostgreSQL" type="Npgsql.NpgsqlFactory, Npgsql, Version=4.1.3.0, Culture=neutral, PublicKeyToken=5d8b90d52f46fda7" />
		</DbProviderFactories>
	</system.data>
</configuration>